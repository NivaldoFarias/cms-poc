{
  "extends": "@cms/tsconfig/client/tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "outDir": "dist",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "src",
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "**/node_modules/**",
    "test",
    "**/*.test.ts"
  ]
}

{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Client",
  "extends": "./../tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "jsx": "preserve",
    "module": "esnext",
    "target": "ES2022",
    "lib": ["dom", "dom.iterable", "esnext"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "skipLibCheck": true
  }
}
